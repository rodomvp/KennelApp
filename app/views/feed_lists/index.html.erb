<p id="notice"><%= notice %></p>

<h1>Listing Feed Lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feed_lists.each do |feed_list| %>
      <tr>
        <td><%= link_to 'Show', feed_list %></td>
        <td><%= link_to 'Edit', edit_feed_list_path(feed_list) %></td>
        <td><%= link_to 'Destroy', feed_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feed list', new_feed_list_path %>
